@model List<Attachment>
@{

}

<div class="jumbotron jumbotron-fluid bg-rsi-blue p-3 mb-3 rounded">
    <div class="container">
        <h1 class="display-6 text-center text-white">Manage Events</h1>
    </div>
</div>

<table class="table table-secondary table-hover table-bordered border-dark">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Link</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (Attachment a in Model)
        {
            <tr>
                <td>@a.ID</td>
                <td>@a.Title</td>
                <td>@a.Link</td>
                <td><button class="btn rsi-btn-red">X</button></td>
            </tr>
        }
    </tbody>
</table>